#==================================================
# 
#
# Dr Jack King.
#
# 3D input files for bubblysph code.
#
# Main code must be compiled with in3d flag for compatibility
# with files generated by this code
#



SHELL=/bin/sh
MAKE=gmake
FC=gfortran

FCFLAGS=-W -O3 -m64 -g -fbounds-check
FCA = -m64

VPATH =../para  
OBJS= kind_parameters.o common_parameter.o global_variables.o datclass.o

default: gen3D

gen3D: $(OBJS)
	$(FC) $(FCA) -o $@ $(OBJS)

%.o: %.F90
	$(FC) $(FCFLAGS) -c -o $@ $<

clean:
	rm -f *.o *.mod gen3D 
	rm -f I*
